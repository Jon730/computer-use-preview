            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "cu:py:main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/computer_use/main.py",
            "args": [
                "--query",
                "'Find the local hp printer device config interface and check the contrasts cyan density is set to -2.  You might need to use the keyboard to navigate inside the dropdown.'",
                "--env",
                "playwright",
                //"--initial_url",
                //"http://192.168.1.124/hp/device/set_config_printDensity.html?tab=System&menu=PrintDensity",
                "--highlight_mouse"
            ],
            "console": "integratedTerminal",
            "python": "/home/vscode/.cache/pypoetry/virtualenvs/computer-use-i9M4nKu9-py3.12/bin/python",
            "env": {
                "USE_VERTEXAI": "true",
                "VERTEXAI_PROJECT": "690897635277",
                "VERTEXAI_LOCATION": "global"  //us-central1"  // ""europe-west2"
            }
        }
    ]
